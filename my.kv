<HomeUser>
    name: "home"
    timee: current


    GridLayout:
        cols:1
        size: root.width, root.height

        row_default_height: 10

        GridLayout:
            cols: 1
            BoxLayout:

                Label:
                    font_size: 33
                    text: "Spotify Chafita"


