                source: 'images/icon.png'



            GridLayout:
                cols: 1
                size_hint_y: None
                height: 10

        GridLayout:
            cols: 2

            GridLayout:
                cols: 1
                size_hint_x: None
                width: 200

                Button:
                    text: "Home"
                    background_normal: 'images/boton2.jpg'
                    background_down: 'images/boton1.jpg'
                Button:
                    text: "Search"
                    background_normal: 'images/boton1.jpg'
                    background_down: 'images/boton2.jpg'
                    on_release:
                        app.root.current = 'search'
                        root.manager.transition.direction = "left"
                Button:
                    text: "Playlists"
                    background_normal: 'images/boton1.jpg'
                    background_down: 'images/boton2.jpg'
                    on_release:
                        app.root.current = 'playlists'
                        root.manager.transition.direction = "left"
                Button:
                    text: "Statistics"
                    background_normal: 'images/boton1.jpg'
                    background_down: 'images/boton2.jpg'
                    on_release:
                        app.root.current = 'stats'
                        root.manager.transition.direction = "left"
                Button:
                    text: "Log Out"
                    background_normal: 'images/boton1.jpg'
                    background_down: 'images/boton2.jpg'
                    on_release:
                        app.root.current = 'log'
                        root.manager.transition.direction = "right"

            GridLayout:
                cols:1

                AnchorLayout:

                    anchor_x: "left"
                    anchor_y: "top"

                    Label:
                        size_hint: (None, None)
                        size: self.texture_size
                        font_size: 20
                        text: "     Welcome!"

                AnchorLayout:

                    anchor_x: "left"
                    anchor_y: "top"

                    Label:
                        size_hint: (None, None)
                        size: self.texture_size
                        text: "     Thank you for using the Spotify Chafita streaming music service"
                        font_size: 17


                AnchorLayout:

                    anchor_x: "left"
                    anchor_y: "top"

                    Label:
                        size_hint: (None, None)
                        size: self.texture_size
                        text: "     To listen to a song, you can either search it by song name, artist or genre"
                AnchorLayout:

                    anchor_x: "left"
                    anchor_y: "top"

                    Label:
                        size_hint: (None, None)
                        size: self.texture_size
                        text: "     You can also listen to a song from your playlist by selecting the 'Playlists' page"

                AnchorLayout:

                    anchor_x: "left"
                    anchor_y: "top"

                    Label:
                        size_hint: (None, None)
                        size: self.texture_size
                        text: "     If you want to susbcribe, you can do it down below"




        GridLayout:
            cols: 1
            Label:
                font_size: 20
                text: "Now Playing:"


            GridLayout:
                cols: 9
                size_hint_y: None
                height: 30

                Label:
                    text: ""

                Label:
                    text: ""
                Label:
                    text: ""

                Button:
                    size_hint_x: None
                    width: 60
                    size_hint_y: None
                    height: 65
                    background_normal: 'images/back.png'
                    pos_hint: {"x": 0.5}

                Button:
                    size_hint_x: None
                    width: 60
                    size_hint_y: None
                    height: 63
                    background_normal: 'images/play.png'
                    pos_hint: {"x": 0.5, "y":0.5}

                Button:
                    size_hint_x: None
                    width: 60
                    size_hint_y: None
                    height: 63
                    background_normal: 'images/next.png'
                    pos_hint: {"x": 0.5}

                Label:
                    text: ""
                Label:
                    text: ""
                Label:
                    text: ""



            GridLayout:
